    "import time\n",
    "\n",
    "def countup(time_sec):\n",
    "    for i in range(time_sec + 1):\n",
    "        mins, secs = divmod(i, 60)\n",
    "        timeformat = '{:02d}:{:02d}'.format(mins, secs)\n",
    "        print(timeformat, end='\\r')\n",
    "        time.sleep(1)\n",
    "    print(\"stop\")\n",
    "\n",
    "countup(90)\n"
   ]
  }
